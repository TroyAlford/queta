{
  "name": "queta",
  "version": "1.0.2",
  "description": "A React component to render J.R.R. Tolkein languages using Glaemscribe",
  "main": "dist/queta-react.js",
  "repository": "git@github.com:TroyAlford/queta.git",
  "author": "Troy Alford <troy@troyalford.com>",
  "license": "AGPL-3.0",
  "files": [
    "dist",
    "source"
  ],
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "scripts": {
    "build": "yarn concurrently -n dev,prod,docs -c yellow,red,grey \"yarn build:dev\" \"yarn build:prod\" \"yarn build:docs\"",
    "build:dev": "yarn webpack",
    "build:docs": "mkdir -p ./dist && cp ./source/docs/* dist/",
    "build:prod": "NODE_ENV=production yarn prebuild && yarn webpack",
    "clean": "rm -rf .cache temp dist",
    "develop": "yarn prebuild && yarn webpack serve",
    "prebuild": "yarn clean && yarn gulp",
    "publish:docs": "git subtree push --prefix dist origin gh-pages",
    "publish:npm": "yarn build:prod && yarn npm publish",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@babel/runtime-corejs3": "^7.16.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-order-alphabetical": "^1.0.1",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "glaemscribe": "./vendor/glaemscribe-v1.2.0-custom.tgz",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    "mini-css-extract-plugin": "^2.5.0",
    "patch-package": "^6.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-csstree-validator": "^2.0.0",
    "stylelint-disallow-at-rule": "^1.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "stylelint-selector-no-empty": "^1.0.8",
